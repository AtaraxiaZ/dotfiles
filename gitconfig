[core]
	editor = vim
    excludesfile = ~/.gitignore_global
[alias]
	graph = log --all --graph --decorate --oneline
[difftool]
    # disables the annoying prompt so whenever you hit 
    # git difftool it will automatically launch the difftool
    prompt = true
[diff]
    tool = vimdiff
[difftool "nvimdiff"]
    cmd = "nvim -d \"$LOCAL\" \"$REMOTE\""
